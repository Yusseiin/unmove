{
  "language": "en",
  "seriesBaseFolders": [
    {
      "name": "Serie TV"
    }
  ],
  "moviesBaseFolders": [
    {
      "name": "Film",
      "alwaysUseFFprobe": true
    }
  ],
  "seriesNamingTemplate": {
    "folderTemplate": "{seriesName} ({seriesYear})",
    "seasonFolderTemplate": "Season {season}",
    "specialsFolderTemplate": "Specials",
    "fileTemplate": "{seriesName} - S{season}E{episode} - {episodeTitle}",
    "seasonPadding": 2,
    "episodePadding": 2
  },
  "movieNamingTemplate": {
    "folderTemplate": "{movieName} ({year}) asd",
    "fileTemplate": "{movieName} ({year}) - {quality} - {codec} - {extraTags}",
    "folderStructure": "year"
  },
  "qualityValues": [
    "2160p",
    "4K",
    "UHD",
    "1080p",
    "1080i",
    "FHD",
    "720p",
    "HD",
    "480p",
    "SD",
    "576p",
    "576i"
  ],
  "codecValues": [
    "x264",
    "x265",
    "AVC",
    "HEVC",
    "10bit",
    "10-bit",
    "HDR",
    "HDR10",
    "HDR10+",
    "DV",
    "Dolby Vision",
    "AV1",
    "VP9"
  ],
  "extraTagValues": [
    "HDR",
    "HDR10",
    "HDR10+",
    "DV",
    "SDR",
    "10bit",
    "10-bit",
    "ITA",
    "AC3"
  ],
  "seriesMetadataProvider": "tvdb",
  "moviesMetadataProvider": "tmdb",
  "metadataProvider": "tmdb"
}